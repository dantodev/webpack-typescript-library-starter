{
  "editor.formatOnSave": false,
  "eslint.validate": ["javascript", "json"],
  "search.exclude": {
    "node_modules": true,
    "dist": true
  },
  "files.watcherExclude": {
    "**/node_modules/*/**": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "jshint.enable": false
}
